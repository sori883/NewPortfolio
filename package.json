{
  "name": "sori883",
  "version": "1.0.1",
  "version_short": "1.0",
  "description": "portfolio",
  "keywords": [
    "sori883"
  ],
  "main": "assets/",
  "scripts": {
    "start": "",
    "css-main": "npm-run-all css-lint css-compile css-prefix css-minify css-copy",
    "css-compile": "node-sass --output-style expanded --source-map true --source-map-contents true src/scss/ -o dist/css/",
    "css-prefix": "postcss --config build/postcss.config.js --replace \"dist/css/*.css\" \"!dist/css/*.min.css\"",
    "css-minify": "cleancss --level 1 --format breakWith=lf --source-map --source-map-inline-sources --output dist/css/style.min.css dist/css/style.css",
    "css-copy": "shx mkdir -p public/assets/css && shx cp -r dist/css/*.min.css public/assets/css",
    "css-lint": "stylelint \"assets/scss/*.scss\"",
    "js-main": "npm-run-all js-compile js-minify js-copy",
    "js-compile": "./node_modules/.bin/browserify js/script.js -o dist/js/script.js",
    "js-minify": "uglifyjs dist/js/script.js -c --source-map --output dist/js/script.min.js",
    "js-copy": "shx mkdir -p public/assets/js && shx cp -r dist/js/*.min.js public/assets/js",
    "watch-css": "nodemon --watch scss/ --ext scss --exec \"npm run css-main\"",
    "server": "browser-sync start --server ./ --startPath public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sori883/NewPortfolio.git"
  },
  "engines": {
    "node": ">= 6"
  },
  "author": "sorinaji",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sori883/NewPortfolio/issues"
  },
  "homepage": "https://sori883.com/",
  "devDependencies": {
    "autoprefixer": "^9.6.1",
    "browser-sync": "^2.26.7",
    "browserify": "^16.4.0",
    "clean-css-cli": "^4.3.0",
    "express": "^4.17.1",
    "jquery": "^3.4.1",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.3",
    "shx": "^0.3.2",
    "stylelint": "^10.1.0",
    "stylelint-config-twbs-bootstrap": "^0.4.0",
    "uglify-js": "^3.6.0"
  }
}

RewriteEngine on
RewriteCond %{HTTPS} off
RewriteRule ^(.*)$ https://sori883.com/$1 [R=301,L]

Options -Indexes

RewriteCond %{HTTPS} !on
RewriteRule (.*) https://%{HTTP_HOST}%{REQUEST_URI} [R=301,L]

<files ~ "^.*\.(&#91;Hh&#93;&#91;Tt&#93;&#91;Aa&#93;)">
order allow,deny
deny from all
satisfy all
</files>
